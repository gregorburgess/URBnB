<%= simple_form_for(@farm) do |f| %>
<div class="form-inputs">
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :location %>
  <%= f.input :country, :as => :string%>
  <%= f.input :city %>
  <%= f.input :price %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
</div>

<div class="form-actions">
  <%= f.button :submit %>
</div>
<% end %>
